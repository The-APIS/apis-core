  "description": "Gateway",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "./node_modules/.bin/nodemon index.js",
    "setup": "npm install",
    "sequelize": "./node_modules/.bin/sequelize $@",
    "migration:generate": "npm run sequelize migration:generate -- $@",
    "model:generate": "npm run sequelize model:generate -- $@",
    "seed:generate": "npm run sequelize seed:generate -- $@",
    "db:migrate": "pm run sequelize db:migrate -- $@",
    "db:seed:all": "npm run sequelize db:seed:all -- $@",
    "seed": "npm run db:seed:all -- $@",
    "test:watch": "./node_modules/mocha/bin/mocha --recursive --watch",
    "test": "./node_modules/mocha/bin/mocha --recursive"
  },
  "keywords": [],
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "0.21.1",
    "body-parser": "1.19.0",
    "connect-redis": "5.0.0",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "debug": "2.6.9",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "express-validator": "6.6.1",
    "lodash": "4.17.20",
    "module-alias": "2.2.0",
    "morgan": "1.9.1",
    "node-fetch": "2.6.1",
    "passport": "0.4.1",
    "passport-github2": "0.1.12",
    "pg": "8.3.2",
    "query-string": "6.6.0",
